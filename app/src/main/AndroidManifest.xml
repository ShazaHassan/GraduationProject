            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.SupportStartUp"
            android:label="Support startup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Reward_campaign_Home_page"
