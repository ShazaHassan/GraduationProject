        android:theme="@style/AppTheme">
        <activity
            android:name=".Template.Main_Template"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Home_Page"
            android:label="@string/graduation_project_name"
